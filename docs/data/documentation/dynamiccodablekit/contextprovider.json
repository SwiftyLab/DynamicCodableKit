ce"},{"type":"text","text":"’s "},{"code":"userInfo","type":"codeVoice"},{"type":"text","text":" property associated to "},{"identifier":"doc:\/\/DynamicCodableKit\/documentation\/DynamicCodableKit\/UserInfoDynamicDecodingContextProvider\/infoKey","type":"reference","isActive":true},{"type":"text","text":"."}],"type":"paragraph"},{"syntax":"swift","code":["extension CodingUserInfoKey {","    static let postDetailDecodingContext = CodingUserInfoKey(rawValue: \"post_detail_decoding_context\")!","}","","struct PostDetailDecodingContextProvider: UserInfoDynamicDecodingContextProvider {","    static var infoKey: CodingUserInfoKey { .postDetailDecodingContext }","}"],"type":"codeListing"},{"inlineContent":[{"type":"text","text":"Finally, the top "},{"type":"codeVoice","code":"Decodable"},{"type":"text","text":" model can be created with "},{"type":"reference","identifier":"doc:\/\/DynamicCodableKit\/documentation\/DynamicCodableKit\/DynamicDecodingContextBasedWrapper","isActive":true},{"type":"text","text":" wrapped property to consume the decoding context provided:"}],"type":"paragraph"},{"syntax":"swift","code":["struct PostDetailPage: Decodable {","    let next: URL","    @DynamicDecodingContextBasedWrapper<PostDetailDecodingContextProvider> var data: PostDetail","}"],"type":"codeListing"},{"inlineContent":[{"type":"text","text":"Before decoding, the context can be provided in the top level decoder’s "},{"type":"codeVoice","code":"userInfo"},{"type":"text","text":" property to be consumed later. If a text post is clicked, then assuming the "},{"type":"codeVoice","code":"TextPostDetail"},{"type":"text","text":" type would be decoded:"}],"type":"paragraph"},{"syntax":"swift","code":["let decoder = JSONDecoder()","decoder.userInfo[.postDetailDecodingContext] = DynamicDecodingContext(decoding: TextPostDetail.self)","let detailPage = try decoder.decode(PostDetailPage.self, from: json)"],"type":"codeListing"}]}],"topicSections":[{"title":"Protocols","identifiers":["doc:\/\/D