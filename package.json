{
  "name": "DynamicCodableKit",
  "version": "1.0.0",
  "summary": "DynamicCodableKit helps you to implement dynamic JSON decoding.",
  "description": "DynamicCodableKit helps you to implement dynamic JSON decoding within the constraints of Swift's sound type system by working on top of Swift's Codable implementations.",
  "homepage": "https://github.com/SwiftyLab/DynamicCodableKit",
  "license": "MIT",
  "author": {
    "name": "Soumya Ranjan Mahunt",
    "email": "soumya.mahunt@gmail.com",
    "url": "https://twitter.com/soumya_mahunt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SwiftyLab/DynamicCodableKit.git"
  },
  "private": true,
  "dependencies": {
    "swiftylab-ci": "github:SwiftyLab/ci"
  },
  "scripts": {
    "build": "npm exec --package=swiftylab-ci -- build.js",
    "xcodebuild": "npm exec --package=swiftylab-ci -- xcodebuild.js",
    "test": "npm exec --package=swiftylab-ci -- test.js",
    "archive": "npm exec --package=swiftylab-ci -- archive.js",
    "pod-lint": "npm exec --package=swiftylab-ci -- pod-lint.js",
    "generate": "npm exec --package=swiftylab-ci -- generate.js --generate-linuxmain",
    "preview-doc": "npm exec --package=swiftylab-ci -- preview-doc.js DynamicCodableKit",
    "build-doc": "npm exec --package=swiftylab-ci -- build-doc.js DynamicCodableKit",
    "serve-doc": "npm exec --package=swiftylab-ci -- serve-doc.js DynamicCodableKit"
  }
}